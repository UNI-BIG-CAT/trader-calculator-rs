{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "trader-calculator",
  "version": "0.1.0",
  "identifier": "com.trader-calculator.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "股算神器",
        "width": 800,
        "height": 800,
        "minHeight": 500,
        "minWidth": 500
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["app"],
    "icon": [
      "icons/icon.png"
    ]
  }
}
